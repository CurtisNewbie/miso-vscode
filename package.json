{
  "name": "miso-vscode",
  "displayName": "miso-vscode",
  "description": "Syntax highlighting for miso",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Programming Languages"
  ],
